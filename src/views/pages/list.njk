{% extends "layouts/main.njk" %}
{% from "macros/tags.njk" import tags %}

{% block title %}
  {{ title | striptags }}
  - {{ super() }}
{% endblock %}

{% block content %}
  <h2
    class="c-page__title"
    property="name"
    id="skip-to-content-link-target"
    tabindex="-1"
  >
    {{ title | safe }}
  </h2>

  <ol>
    {% for item in list %}
      <li>
        <a href="{{ item.url }}">{{ item.label }}</a>
        {{ item.append }}
      </li>
    {% endfor %}
  </ol>
{% endblock %}
